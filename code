    "                                        learning_rate=LR,\n",
    "                                        lr_scheduler_type=LR_SCHEDULER,\n",
    "                                        save_strategy=SAVE_STRATEGY,\n",
    "                                        logging_steps=LOGGING_STEPS,\n",
    "                                        num_train_epochs=100,\n",
    "                                        # max_steps=MAX_STEPS,\n",
    "                                        fp16=FP16,\n",
    "                                        evaluation_strategy = \"steps\",\n",
    "                                        \n",
    "                                        eval_steps=100, \n",
    "                                        load_best_model_at_end=True,\n",
    "                                        \n",
    "                                        push_to_hub=PUSH_TO_HUB)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def prepare_data_for_training(dataset):\n",
    "    \"\"\"\n",
    "    准备数据集，将其转换为模型期望的格式。\n",
    "    \n",
    "    参数:\n",
    "    - dataset: 包含多个{'chat': [{'content': str, 'role': str}]}结构的数据集\n",
    "\n",
    "    返回:\n",
    "    - 转换后的数据集，只包含文本字符串列表\n",
    "    \"\"\"\n",
    "    prepared_data = []\n",
    "    for item in dataset:\n",
    "        # 提取每个'chat'项中的'content'字段，并将其合并为单个字符串\n",
    "        chat_contents = [chat['content'] for chat in item['chat']]\n",
    "        prepared_data.append(\" \".join(chat_contents))\n",
    "    return prepared_data\n",
    "# 假设您的原始数据集是data_train和data_test\n",
    "data_train_prepared = prepare_data_for_training(data_train)\n",
    "data_test_prepared = prepare_data_for_training(data_test)\n",
    "\n",
    "# Assuming data_train_prepared and data_test_prepared are lists of strings\n",
    "train_dataset = Dataset.from_dict({\"text\": data_train_prepared})\n",
    "eval_dataset = Dataset.from_dict({\"text\": data_test_prepared})"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": []
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "trainer = SFTTrainer(\n",
    "                        model=model,\n",
    "                        train_dataset=train_dataset,\n",
    "                        eval_dataset = eval_dataset,\n",
    "                        peft_config=peft_config,\n",
    "                        dataset_text_field = \"text\",\n",
    "                        args=training_arguments,\n",
    "                        \n",
    "                        \n",
    "                        tokenizer=tokenizer,\n",
    "                        packing=PACKING,\n",
    "                        max_seq_length=MAX_SEQ_LENGTH,\n",
    "                        \n",
    "                    )\n",
    "trainer.train()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# from transformers import EvalPrediction\n",
    "# training_arguments = TrainingArguments(\n",
    "#     output_dir=OUTPUT_DIR,\n",
    "#     num_train_epochs=1,\n",
    "#     per_device_train_batch_size=BATCH_SIZE,\n",
    "#     per_device_eval_batch_size=BATCH_SIZE,\n",
    "#     warmup_steps=500,\n",
    "#     weight_decay=0.01,\n",
    "#     logging_dir='./logs',  # 用于TensorBoard日志\n",
    "#     logging_steps=50,\n",
    "#     evaluation_strategy=\"steps\",  # 在训练过程中启用评估\n",
    "#     eval_steps=100,  # 每100步评估一次\n",
    "#     save_strategy=\"steps\",\n",
    "#     load_best_model_at_end=True,\n",
    "# )\n",
    "# def compute_metrics(p: EvalPrediction):\n",
    "#     # 假设p.predictions和p.label_ids都是numpy数组\n",
    "#     # 在CLM任务中，p.predictions可能是模型的原始输出，需要进一步处理以计算具体指标\n",
    "#     # 这里我们简化为直接利用损失值，通常在评估时需要您自行定义如何从模型预测中计算出指标\n",
    "    \n",
    "#     # 计算平均损失，这里简单示例，具体计算方式根据实际情况可能需要调整\n",
    "#     loss = p.predictions[0] if isinstance(p.predictions, tuple) else p.predictions\n",
    "#     avg_loss = loss.mean()\n",
    "\n",
    "#     return {\n",
    "#         \"avg_loss\": avg_loss,  # 返回平均损失\n",
    "#     }\n",
    "\n",
    "# trainer = SFTTrainer(\n",
    "#     model=model,\n",
    "#     args=training_arguments,\n",
    "#     train_dataset=train_dataset,\n",
    "#     dataset_text_field = \"text\",\n",
    "#     ###这个text是代表着我的数据集前面的那个索引\n",
    "#     eval_dataset=eval_dataset,\n",
    "#     tokenizer=tokenizer,\n",
    "#     compute_metrics=compute_metrics,  # 可以定义一个函数来计算和返回评估指标\n",
    "# )\n",
    "\n",
